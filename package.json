{
  "name": "@dcorrea-estrav/vuejs-autocomplete",
  "version": "2.1.7",
  "description": "Autocomplete Component for Vue 2",
  "main": "./dist/vue2-autocomplete.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/webpack.config.js --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack-prod.config.js --mode production",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack/webpack-bundle.config.js --mode production",
    "build:dist": "cross-env NODE_ENV=development webpack --config ./webpack/webpack-bundle.config.js",
    "prepublish": "npm run build && npm run build:prod && npm run build:dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dcorrea-estrav/vue2-autocomplete.git"
  },
  "keywords": [
    "vuejs",
    "autocomplete",
    "remote-list",
    "form",
    "component",
    "vuejs-autocomplete",
    "form-helper",
    "vue-2"
  ],
  "author": "Daniel H. <strike970124@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dcorrea-estrav/vue2-autocomplete/issues"
  },
  "homepage": "https://github.com/dcorrea-estrav/vue2-autocomplete#readme",
  "devDependencies": {
    "babel-loader": "7",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "es6-promise": "^4.2.8",
    "extract-text-webpack-plugin": "^3.0.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-group-css-media-queries": "^1.2.0",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-string-replace": "^1.1.2",
    "jsx-control-statements": "^3.2.5",
    "postcss-loader": "^5.3.0",
    "style-loader": "^2.0.0",
    "vue-loader": "^15.9.7",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.1.11"
  },
  "dependencies": {
    "vue": "^2.4.2"
  }
}
